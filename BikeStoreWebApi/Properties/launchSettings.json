      "publishAllPorts": true,
      "httpPort": 5000,
      "useSSL": true,
      "sslPort": 5001
    },
    "BikeStoreWenApi (Prod)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
